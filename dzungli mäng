import pygame

pygame.init()

laius = 626
kõrgus = 357
aken = pygame.display.set_mode((laius, kõrgus))

pygame.display.set_caption("Džunglimäng")

käib = True
while käib:
    
    for event in pygame.event.get():
        if event.type == pygame.QUIT:
            käib = False
     
    pygame.display.flip()

pygame.quit()
